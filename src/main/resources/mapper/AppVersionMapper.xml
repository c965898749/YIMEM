<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sy.mapper.game.AppVersionMapper">

    <select id="selectListLast" resultType="com.sy.model.game.AppVersion">
        SELECT * from app_version
        ORDER BY app_version.version
        limit 1
    </select>
</mapper>